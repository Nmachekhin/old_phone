#include <PCM.h>
#include <SoftwareSerial.h>




const short int HANG_PIN=2;
const short int DIAL_PIN=3;

unsigned long global_timer;


unsigned long status_timer;
const unsigned long status_delay=250;

unsigned long dial_timer;
unsigned long dial_standby_timer;
const unsigned long dial_btn_delay=250;
const unsigned long dial_before_call_delay=4000;
bool last_btn_state=false;
String dialed_number="";
short current_number=0;


SoftwareSerial sim800l(7, 8);

enum MainStatus{
  UNKNOWN_STATUS=0,
  IDDLE=1,
  DIALING=2,
  CALLING=3,
  SPEAKING=4,
  OPONENT_HANGED=5
};

MainStatus status=UNKNOWN_STATUS;

//AT+CPAS
enum SimStatus {
  READY=0,
  UNKNOWN=2,
  INCOMMING=3,
  INCALL=4
};

SimStatus sim_status=UNKNOWN;

void setup() {
  Serial.begin(9600);
  sim800l.begin(9600);
  sim800l.println("at+sndlevel=0,100");
  sim800l.println("at+cmic=0,15");
  sim800l.println("atl9");
  pinMode(HANG_PIN, INPUT_PULLUP);
  pinMode(DIAL_PIN, INPUT_PULLUP);
  Serial.println("Initialization is complete!");
  global_timer=millis();
  status_timer=0;
}


bool hanged()
{
    return !digitalRead(HANG_PIN);
}

void to_iddle()
{
  sim800l.println("at+sttone=1,1,200");
  sim800l.println("ath");
  
  status=IDDLE;
  dialed_number="";
  Serial.println("hanged");
}

void answer()
{
  sim800l.println("AT+STTONE=0");
  status=SPEAKING;
  sim800l.println("ATA");
}


void loop() {
  global_timer=millis();

  if (hanged() && status!=IDDLE) to_iddle();
  if (hanged() && sim_status==INCOMMING)
  {
    play_ringthone();
  }
  if (!hanged() && status==IDDLE && sim_status==READY) 
  {
    status=DIALING;
    sim800l.println("AT+STTONE=1,1,15200000");
    dial_timer=global_timer;
    dial_standby_timer=global_timer;
  }
  if (!hanged() && status==SPEAKING && sim_status==READY)
  {
    status = OPONENT_HANGED;
    sim800l.println("AT+STTONE=1,3,15200000");
  }
  if (!hanged() && status==IDDLE && sim_status==INCOMMING) answer();
  if(!hanged() && status==DIALING) operate_dial();
  if (status==IDDLE || status==SPEAKING)
    check_status();
  /*Serial.print(status);
  Serial.print(" ");
  Serial.println(sim_status);*/
}


void call()
{
    sim800l.println("at+sttone=1,1,200");
    sim800l.println("ath");
    sim800l.println("atd"+dialed_number+";");
    status=SPEAKING;
    sim_status=INCALL;
}


void operate_dial()
{
  
  if(global_timer-dial_timer<dial_btn_delay && digitalRead(DIAL_PIN)!=last_btn_state)
  {
    last_btn_state=digitalRead(DIAL_PIN);

    if (last_btn_state)
    {
      current_number++;
    }
    sim800l.println("AT+STTONE=0");
    dial_timer=global_timer;
  }else if (global_timer-dial_timer>=dial_btn_delay)
  {
    
    last_btn_state=digitalRead(DIAL_PIN);
    if(current_number>0 && dialed_number.length()<15)
    {
      dialed_number+=current_number%10;
      dial_standby_timer=global_timer;
    }
    current_number=0;
    dial_timer=global_timer;
  }
  if(global_timer-dial_standby_timer>=dial_before_call_delay && dialed_number!="") call();
  Serial.println(dialed_number);
  manual_sim_control();
}







void manual_sim_control()
{
  while (Serial.available()) 
  {
    sim800l.write(Serial.read());
  }
  while(sim800l.available()) 
  {
    Serial.write(sim800l.read());
  }
}

void check_status()
{
  if(global_timer-status_timer>=status_delay)
  {
    status_timer=global_timer;
    
    sim800l.println("AT+CPAS");
      String response="";
    while(!sim800l.available()); 
    while(sim800l.available()) 
    {
      response+=sim800l.readString();
    }
    Serial.println(response);
    if (response.indexOf("+CPAS: 0") >=0 )
      sim_status=READY;
    else if (response.indexOf("+CPAS: 2") >=0 )
      sim_status=UNKNOWN;
    else if (response.indexOf("+CPAS: 3") >=0 )
      sim_status=INCOMMING;
    else if (response.indexOf("+CPAS: 4") >=0 )
      sim_status=INCALL;

  }


}

